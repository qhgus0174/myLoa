{
    "name": "myloaschedule",
    "version": "1.0.0",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.21.4"
    },
    "engines": {
        "node": "14.17.5",
        "yarn": "1.22.11"
    },
    "scripts": {
        "start": "node dist/src/index.js",
        "postinstall": "tsc & tsc bin/weekcontents.ts",
        "heroku-postbuild": "cd bin && yarn install && cd ../client && npm install --dev && yarn build",
        "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\" ",
        "dev:client": "cd client && yarn dev",
        "dev:server": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts"
    }
}
